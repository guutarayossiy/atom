"*":
  "atom-beautify":
    general:
      _analyticsUserId: "22ba7866-8b6d-4a03-bd8d-470ee6ec6f4d"
  "atom-material-ui":
    fonts: {}
    ui:
      animations: false
  autosave: {}
  core:
    audioBeep: false
