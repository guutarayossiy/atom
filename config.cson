      defaultPanelHeight: "200px"
  preview:
    _analyticsUserId: "5f7e724f-ba7b-4057-ae39-379b09c71a09"
  "project-manager":
    sortBy: "group"
  "seti-ui":
    compactView: true
    disableAnimations: true
  "status-bar":
    fullWidth: false
  "tree-view": {}
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
".css.scss.source":
  editor:
    autoIndentOnPaste: true
".ini.source":
  editor:
    autoIndentOnPaste: false
    showIndentGuide: true
    softWrap: true
".sass.source":
  editor:
    autoIndentOnPaste: true
