      "minimap-autohide"
      "minimap-bookmarks"
      "minimap-highlight-selected"
      "terminal-plus"
      "pigments"
      "bookmarks"
      "atom-autocomplete-php"
      "file-icons"
    ]
    telemetryConsent: "limited"
    themes: [
      "seti-ui"
      "monokai"
    ]
  editor:
    autoIndentOnPaste: false
    fontFamily: "\"Source Han Code JP\""
    fontSize: 13
    invisibles: {}
    showInvisibles: true
    softWrap: true
  emmet: {}
  "exception-reporting":
    userId: "00ba6670-a453-69d8-b805-8d447dd849ad"
  "genesis-ui":
    distractionFree:
      hideBottom: false
      hideFiles: false
      hideTabs: false
    treeView: {}
  "git-plus":
    general:
      _analyticsUserId: "64a6184f-c869-47ea-92db-3348a061dddc"
  "highlight-line":
    enableBackgroundColor: false
  minimap:
    displayCodeHighlights: false
